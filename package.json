{
	"name": "duo-market-data",
	"version": "1.0.9",
	"description": "DUO Market Data",
	"author": "Duo Network",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"test": "jest",
		"prebuild": "rimraf dist",
		"build": "tsc",
		"prepare": "npm run build",
		"tslint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts{,x} --fix"
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"devDependencies": {
		"@babel/polyfill": "^7.4.4",
		"@types/jest": "^24.0.17",
		"coveralls": "^3.0.6",
		"jest": "^24.8.0",
		"pre-commit": "^1.2.2",
		"rimraf": "^2.6.3",
		"ts-jest": "^24.0.2",
		"tslint": "^5.18.0",
		"typescript": "^3.5.3"
	},
	"dependencies": {
		"@finbook/duo-contract-wrapper": "^1.5.2",
		"@types/d3-format": "^1.3.1",
		"aws-sdk": "^2.508.0",
		"d3-format": "^1.3.2",
		"moment": "^2.29.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FinBook/duo-market-data.git"
	},
	"keywords": [
		"DUO",
		"Market",
		"Data"
	],
