		"pre-commit": "^1.2.2",
		"rimraf": "^2.6.3",
		"ts-jest": "^23.10.5",
		"tslint": "^5.12.1",
		"typescript": "^3.1.6"
	},
	"dependencies": {
		"@finbook/duo-contract-wrapper": "^1.1.0",
